SET DATABASE COLLATION "German"
CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE CACHED TABLE "Objekt"("idObjekt" INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,"signatur" VARCHAR(100),"titel" VARCHAR(100))
CREATE CACHED TABLE "Buch"("idObjekt" INTEGER NOT NULL PRIMARY KEY,"autor" VARCHAR(100),"verlag" VARCHAR(100))
CREATE CACHED TABLE "DVD"("idObjekt" INTEGER NOT NULL PRIMARY KEY,"fsk" INTEGER,"jahr" DATE)
CREATE CACHED TABLE "Nutzer"("idNutzer" INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,"vorname" VARCHAR(100),"nachname" VARCHAR(100),"wohnort" VARCHAR(100),"strasse" VARCHAR(100),"hausnummer" CHAR(4),"plz" CHAR(5))
CREATE CACHED TABLE "Ausleihe"("idNutzer" INTEGER NOT NULL,"idObjekt" INTEGER NOT NULL,"von" DATE NOT NULL,"bis" DATE,PRIMARY KEY("idNutzer","idObjekt","von"))
SET TABLE "Objekt" INDEX'176 6'
SET TABLE "Buch" INDEX'568 0'
SET TABLE "DVD" INDEX'712 0'
SET TABLE "Nutzer" INDEX'776 2'
SET TABLE "Ausleihe" INDEX'1008 0'
ALTER TABLE "Objekt" ALTER COLUMN "idObjekt" RESTART WITH 6
ALTER TABLE "Nutzer" ALTER COLUMN "idNutzer" RESTART WITH 2
CREATE VIEW "Ansicht" ("idNutzer","vorname","nachname","titel") AS SELECT \u000a  "Ausleihe"."idNutzer", "vorname", "nachname", "titel" \u000aFROM \u000a  "Ausleihe", "Nutzer", "Objekt" \u000aWHERE \u000a  "Ausleihe"."idNutzer" = "Nutzer"."idNutzer" \u000a  AND "Objekt"."idObjekt" = "Ausleihe"."idObjekt"
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 0 MILLIS
